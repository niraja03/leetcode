
#include <iostream>
using namespace std;

int mystr(int arr[], int n, int target) {
    for(int i=0; i<n ; i ++){
        if(arr[i]==target) return i;
    }
    return -1;
}

int main() {
    int arr[] = {1,2,3,4,5};
    int n =5;
    int target =5;
    int result = mystr(arr, n, target);
    
    if(result != -1) {
        cout<< "element found at index"<< " "<< result <<endl;
    }
    else cout<<"could not find";
    
    return 0;
}
