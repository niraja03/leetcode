
#include <bits/stdc++.h>
using namespace std;

void unionofarr(int arr1[], int arr2[], int n1, int n2){
    set<int> st;
    for(int i=0; i<n1 ; i++) {
        st.insert(arr1[i]) ;
    }
    for(int i=0 ; i<n2 ; i++) {
        st.insert(arr2[i]) ;
    }
    
    for(int x : st ) {
        cout<< x<< " ";
    }
    cout<<endl;
}

int main() {
    int arr1[] = {1,1,2,3,3,4};
    int arr2[] ={1,4,4,6,7,7}; // U would be 1,2,3,4,6,7
    int n1= 6;
    int n2= 6;
    unionofarr(arr1, arr2, n1, n2);
    
    return 0;
}
